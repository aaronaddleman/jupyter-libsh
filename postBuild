mkdir $HOME/src
mkdir $HOME/bin
git clone https://github.com/aaronaddleman/libsh.git $HOME/src/libsh
cp $HOME/src/libsh/.libshrc $HOME/.libshrc
curl -SsL https://github.com/cdr/code-server/releases/download/3.1.1/code-server-3.1.1-linux-x86_64.tar.gz | tar -C $HOME/bin -xzf -

[ -f $HOME/cmd.sh ] && bash cmd.sh
